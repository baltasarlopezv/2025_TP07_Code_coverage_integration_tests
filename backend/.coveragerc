[run]
source = app
omit =
    */tests/*
    */venv/*
    app/routes/*
    app/init_db.py
relative_files = True

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[xml]
output = coverage.xml

[html]
directory = htmlcov
